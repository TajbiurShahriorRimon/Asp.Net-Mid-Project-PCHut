@model IEnumerable<PcHut.Models.user>

@{
    ViewBag.Title = "ProductBoughtByBuyers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ProductBoughtByBuyers</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.user_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.registration_date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.user_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.registration_date)
        </td>
        <td>
           
            @Html.ActionLink("Details", "Details", new { id=item.user_id }) |
      
        </td>
    </tr>
}

</table>
